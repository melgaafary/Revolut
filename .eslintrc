{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "plugins": [
    "react-hooks"
  ],
  "globals": {
    jest: true,
    it: true,
    expect: true,
    describe: true,
    fetch: true
  }
}
